{"ast":null,"code":"import { reactive, ref, onMounted, inject } from 'vue';\nimport Chessboard from \"@/components/chessboard\";\nexport default {\n  name: \"Gobang\",\n  components: {\n    Chessboard\n  },\n\n  setup() {\n    const global = inject('global');\n    const axios = inject('axios');\n    /*\r\n    * page_num:\r\n    * 1 选择创建or加入\r\n    * 2 创建页面\r\n    * 3 加入页面\r\n    * 4 棋盘页面\r\n    * */\n\n    const radioStyle = reactive({\n      display: 'flex',\n      height: '30px',\n      lineHeight: '30px'\n    });\n    const room_num = ref(\" \");\n    room_num.value = ''; //棋盘号\n\n    let chessboard_num = -1;\n    let xianshouOrhoushou = ref(true); //创建棋盘函数\n\n    function createChessboard() {\n      const create_url = global.http_local_url + 'gobang/createChessboard/' + global.username + '/' + xianshouOrhoushou;\n      console.log(create_url);\n      axios.get(create_url).then(res => {\n        const resData = res.data;\n        console.log(resData);\n\n        if (resData.code === 0) {\n          chessboard_num = resData.data.chessboard_num;\n        }\n      });\n    }\n\n    return {\n      page_num: ref(1),\n      radioStyle,\n      xianshouOrhoushou,\n      room_num,\n      createChessboard\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,SAAQA,QAAR,EAAkBC,GAAlB,EAAuBC,SAAvB,EAAkCC,MAAlC,QAA+C,KAA/C;AACA,OAAOC,UAAP,MAAuB,yBAAvB;AACA,eAAe;EACbC,IAAI,EAAE,QADO;EAEbC,UAAU,EAAE;IAACF;EAAD,CAFC;;EAGbG,KAAK,GAAE;IAEL,MAAMC,MAAM,GAACL,MAAM,CAAC,QAAD,CAAnB;IACA,MAAMM,KAAK,GAACN,MAAM,CAAC,OAAD,CAAlB;IAEA;;;;;;;;IAOA,MAAMO,UAAS,GAAIV,QAAQ,CAAC;MAC1BW,OAAO,EAAE,MADiB;MAE1BC,MAAM,EAAE,MAFkB;MAG1BC,UAAU,EAAE;IAHc,CAAD,CAA3B;IAKA,MAAMC,QAAQ,GAACb,GAAG,CAAC,GAAD,CAAlB;IACAa,QAAQ,CAACC,KAAT,GAAe,EAAf,CAlBK,CAmBL;;IACA,IAAIC,cAAc,GAAC,CAAC,CAApB;IACA,IAAIC,iBAAiB,GAAChB,GAAG,CAAC,IAAD,CAAzB,CArBK,CAuBL;;IACA,SAASiB,gBAAT,GAA2B;MACzB,MAAMC,UAAU,GAACX,MAAM,CAACY,cAAP,GAAsB,0BAAtB,GAAiDZ,MAAM,CAACa,QAAxD,GAAiE,GAAjE,GAAqEJ,iBAAtF;MACAK,OAAO,CAACC,GAAR,CAAYJ,UAAZ;MACAV,