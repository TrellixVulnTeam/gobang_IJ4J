{"ast":null,"code":"import { reactive, inject } from 'vue';\nexport default {\n  name: \"Login\",\n\n  setup() {\n    const global = inject('global');\n    const axios = inject('axios');\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true\n    });\n\n    const onFinish = values => {\n      console.log(global);\n      const url = global.http_local_url + 'log/login/' + formState.username + '/' + formState.password + '/' + formState.remember;\n      console.log(url);\n      axios.get(url).then(res => {\n        console.log('请求成功', res.data);\n      });\n      console.log('Success:', values);\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    return {\n      formState,\n      onFinish,\n      onFinishFailed\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwCA,SAAUA,QAAV,EAAmBC,MAAnB,QAAiC,KAAjC;AACA,eAAe;EACbC,IAAI,EAAC,OADQ;;EAEbC,KAAK,GAAG;IACN,MAAMC,MAAM,GAACH,MAAM,CAAC,QAAD,CAAnB;IACA,MAAOI,KAAK,GAACJ,MAAM,CAAC,OAAD,CAAnB;IACA,MAAMK,SAAQ,GAAIN,QAAQ,CAAC;MACzBO,QAAQ,EAAE,EADe;MAEzBC,QAAQ,EAAE,EAFe;MAGzBC,QAAQ,EAAE;IAHe,CAAD,CAA1B;;IAMA,MAAMC,QAAO,GAAIC,MAAK,IAAK;MAEzBC,OAAO,CAACC,GAAR,CAAYT,MAAZ;MACA,MAAMU,GAAG,GAACV,MAAM,CAACW,cAAP,GAAsB,YAAtB,GAAmCT,SAAS,CAACC,QAA7C,GAAsD,GAAtD,GAA0DD,SAAS,CAACE,QAApE,GAA6E,GAA7E,GAAiFF,SAAS,CAACG,QAArG;MACAG,OAAO,CAACC,GAAR,CAAYC,GAAZ;MACAT,KAAK,CAACW,GAAN,CAAUF,GAAV,EAAeG,IAAf,CAAoBC,GAAG,IAAE;QACvBN,OAAO,CAACC,GAAR,CAAY,MAAZ,EAAmBK,GAAG,CAACC,IAAvB;MACD,CAFD;MAIAP,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBF,MAAxB;IACD,CAVD;;IAYA,MAAMS,cAAa,GAAIC,SAAQ,IAAK;MAClCT,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBQ,SAAvB;IACD,CAFD;;IAGA,OAAO;MACLf,SADK;MAELI,QAFK;MAGLU;IAHK,CAAP;EAKD;;AA/BY,CAAf","names":["reactive","inject","name","setup","global","axios","formState","username","password","remember","onFinish","values","console","log","url","http_local_url","get","then","res","data","onFinishFailed","errorInfo"],"sourceRoot":"","sources":["D:\\proj\\wuziqi\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <div id=\"before\"></div>\r\n  <div id=\"form\">\r\n    <a-form\r\n        :model=\"formState\"\r\n        name=\"basic\"\r\n        :label-col=\"{ span: 8 }\"\r\n        :wrapper-col=\"{ span: 16 }\"\r\n        autocomplete=\"off\"\r\n        @finish=\"onFinish\"\r\n        @finishFailed=\"onFinishFailed\"\r\n    >\r\n      <a-form-item\r\n          label=\"Username\"\r\n          name=\"username\"\r\n          :rules=\"[{ required: true, message: 'Please input your username!' }]\"\r\n      >\r\n        <a-input v-model:value=\"formState.username\" />\r\n      </a-form-item>\r\n      <a-form-item\r\n          label=\"Password\"\r\n          name=\"password\"\r\n          :rules=\"[{ required: true,message: 'Please input your password'}]\"\r\n      >\r\n        <a-input-password v-model:value=\"formState.password\" />\r\n      </a-form-item>\r\n\r\n      <a-form-item name=\"remember\" :wrapper-col=\"{ offset: 8, span: 16 }\">\r\n        <a-checkbox v-model:checked=\"formState.remember\">Remember me</a-checkbox>\r\n      </a-form-item>\r\n\r\n      <a-form-item :wrapper-col=\"{ offset: 8, span: 16 }\">\r\n        <a-button type=\"primary\" html-type=\"submit\">Login</a-button>\r\n      </a-form-item>\r\n    </a-form>\r\n  </div>\r\n\r\n  <div id=\"after\"></div>\r\n</template>\r\n<script>\r\nimport {  reactive,inject } from 'vue';\r\nexport default({\r\n  name:\"Login\",\r\n  setup() {\r\n    const global=inject('global');\r\n    const  axios=inject('axios');\r\n    const formState = reactive({\r\n      username: '',\r\n      password: '',\r\n      remember: true,\r\n    });\r\n\r\n    const onFinish = values => {\r\n\r\n      console.log(global)\r\n      const url=global.http_local_url+'log/login/'+formState.username+'/'+formState.password+'/'+formState.remember;\r\n      console.log(url)\r\n      axios.get(url).then(res=>{\r\n        console.log('请求成功',res.data);\r\n      })\r\n\r\n      console.log('Success:', values);\r\n    };\r\n\r\n    const onFinishFailed = errorInfo => {\r\n      console.log('Failed:', errorInfo);\r\n    };\r\n    return {\r\n      formState,\r\n      onFinish,\r\n      onFinishFailed,\r\n    };\r\n  },\r\n\r\n});\r\n</script>\r\n<style>\r\n#before{\r\n  width: 33%;\r\n  height: 100%;\r\n  display: inline-block;\r\n}\r\n#form{\r\n  display: inline-block;\r\n}\r\n#after{\r\n  width: 33%;\r\n  height: 100%;\r\n  display: inline-block;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}