{"ast":null,"code":"import { reactive, getCurrentInstance } from 'vue';\nexport default {\n  name: \"Login\",\n  interface: 'log/login/',\n\n  setup() {\n    const formState = reactive({\n      username: '',\n      password: '',\n      remember: true\n    });\n\n    const onFinish = values => {\n      const {\n        appContext: {\n          config: {\n            globalProperties\n          }\n        }\n      } = getCurrentInstance();\n      const url = globalProperties.$global.http_local_url + formState.username + '/' + formState.password + '/' + formState.remember;\n      this.$axios.get(url).then(res => {\n        console.log('请求成功', res.data);\n      });\n      console.log('Success:', values);\n    };\n\n    const onFinishFailed = errorInfo => {\n      console.log('Failed:', errorInfo);\n    };\n\n    return {\n      formState,\n      onFinish,\n      onFinishFailed,\n      onSubmitted\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwCA,SAAUA,QAAV,EAAmBC,kBAAnB,QAA6C,KAA7C;AACA,eAAe;EACbC,IAAI,EAAC,OADQ;EAEbC,SAAS,EAAC,YAFG;;EAGbC,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIL,QAAQ,CAAC;MACzBM,QAAQ,EAAE,EADe;MAEzBC,QAAQ,EAAE,EAFe;MAGzBC,QAAQ,EAAE;IAHe,CAAD,CAA1B;;IAMA,MAAMC,QAAO,GAAIC,MAAK,IAAK;MACzB,MAAK;QAACC,UAAU,EAAC;UAACC,MAAM,EAAE;YAAEC;UAAF;QAAT;MAAZ,IAA6CZ,kBAAkB,EAApE;MACA,MAAMa,GAAG,GAACD,gBAAgB,CAACE,OAAjB,CAAyBC,cAAzB,GAAwCX,SAAS,CAACC,QAAlD,GAA2D,GAA3D,GAA+DD,SAAS,CAACE,QAAzE,GAAkF,GAAlF,GAAsFF,SAAS,CAACG,QAA1G;MACA,KAAKS,MAAL,CAAYC,GAAZ,CAAgBJ,GAAhB,EAAqBK,IAArB,CAA0BC,GAAG,IAAE;QAC7BC,OAAO,CAACC,GAAR,CAAY,MAAZ,EAAmBF,GAAG,CAACG,IAAvB;MACD,CAFD;MAIAF,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBZ,MAAxB;IACD,CARD;;IAUA,MAAMc,cAAa,GAAIC,SAAQ,IAAK;MAClCJ,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBG,SAAvB;IACD,CAFD;;IAGA,OAAO;MACLpB,SADK;MAELI,QAFK;MAGLe,cAHK;MAILE;IAJK,CAAP;EAMD;;AA7BY,CAAf","names":["reactive","getCurrentInstance","name","interface","setup","formState","username","password","remember","onFinish","values","appContext","config","globalProperties","url","$global","http