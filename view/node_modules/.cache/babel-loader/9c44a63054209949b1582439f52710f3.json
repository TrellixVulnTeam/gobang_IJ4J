sword+'/'+formState.remember;\r\n      this.$axios.get(url).then(res=>{\r\n        console.log('请求成功',res.data);\r\n      })\r\n\r\n      console.log('Success:', values);\r\n    };\r\n\r\n    const onFinishFailed = errorInfo => {\r\n      console.log('Failed:', errorInfo);\r\n    };\r\n    return {\r\n      formState,\r\n      onFinish,\r\n      onFinishFailed,\r\n      onSubmitted,\r\n    };\r\n  },\r\n\r\n});\r\n</script>\r\n<style>\r\n#before{\r\n  width: 33%;\r\n  height: 100%;\r\n  display: inline-block;\r\n}\r\n#form{\r\n  display: inline-block;\r\n}\r\n#after{\r\n  width: 33%;\r\n  height: 100%;\r\n  display: inline-block;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}